df <- unlist(read.csv("bu.csv", header = F, sep = ";"), use.names = F)
a0 <- 5.5
alpha  <-  0.1
sigma0 <- 2.4
a1 <- 5
sigma1 <- 2.5
eps <- 0.2
n <- 90
x1 <- rnorm(n = 1e5, mean = a0, sd = sigma1)
